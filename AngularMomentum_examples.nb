  RowBox[{
   RowBox[{"(", 
    RowBox[{"-", "1"}], ")"}], "^", 
   RowBox[{"(", 
    RowBox[{"fac1", "+", 
     RowBox[{"2", "fac2"}], "+", 
     RowBox[{"3", "fac3"}], "-", "fac4"}], ")"}]}], "//.", 
  "simplifyFactorRules"}]], "Input",
