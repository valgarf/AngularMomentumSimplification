   3.606191613413471*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"removeBlanks", "[", 
  RowBox[{"f", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a_", "+", "b__"}], ")"}], "^", "2"}], ",", "c___"}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.606191547047276*^9, 3.606191597563265*^9}}],

Cell[BoxData[
